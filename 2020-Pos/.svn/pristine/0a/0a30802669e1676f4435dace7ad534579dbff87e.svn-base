<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/mainMatchColor_80" />
                    <corners android:radius="4dp" />
                    <size  android:height="44dp"
                        android:width="84dp"/>
                    <stroke android:color="#D9D9D9"
                        android:width="1dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/mainMatchColor" />
            <corners android:radius="4dp" />
            <size  android:height="44dp"
                android:width="84dp"/>
            <stroke android:color="#D9D9D9"
                android:width="1dp"/>
        </shape>
    </item>

</selector>