<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bottomColor">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:padding="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_launcher"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/in"
            android:textColor="@color/mainTextColor"
            android:textSize="13sp"
            android:layout_gravity="center"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/qualification_information_subject_qualification"
            android:textColor="@color/mainColor"
            android:textSize="13sp"
            android:layout_gravity="center"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="12dp"
            android:text="@string/your_business_registration_certificate_is_about_to_expire"
            android:textColor="@color/mainTextColor"
            android:textSize="13sp"
            android:layout_gravity="center"
            />

        <TextView
            android:id="@+id/tvEdit"
            android:layout_marginLeft="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/edit_person_info"
            android:layout_gravity="center"
            android:layout_marginRight="12dp"
            android:textSize="13sp"
            android:textColor="@color/blue"
            android:gravity="end" />
    </LinearLayout>

    <LinearLayout
        android:padding="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="*"
            android:textColor="@color/mainColor"
            android:textSize="13sp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13sp"
            android:textColor="@color/mainTextColor"
            android:text=""/>
    </LinearLayout>
    <TextView
        android:padding="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/base_info"
        android:textColor="@color/mainTextColor"
        android:textSize="18sp"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
               style="@style/BusinessInfo"
                android:text="@string/store_id"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/store_name"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/Industry_name"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/address_detail"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/contract_person"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/phone_num"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/email_address"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <TextView
            android:layout_marginLeft="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/mainTextColor"
            android:textSize="18sp"
            android:text="@string/qualification_info"/>
        <TextView
            android:padding="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/subject_qualification"
            android:textSize="16sp"
            android:textColor="@color/mainTextColor"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/type_of_certificate"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text="@string/business_registration_certificate"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/id_card_photo"
                />
            <ImageView
                style="@style/BusinessInfo2"
                android:src="@mipmap/ic_launcher"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/validity_of_documents"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text="2024/08/09"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/registration_number"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/legal_representative"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/identification_number"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
       <TextView
           android:padding="12dp"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:text="@string/legal_representative_information"
           android:textColor="@color/mainTextColor"
           android:textSize="16sp"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/handheld_id_card_front"
                />
            <ImageView
                style="@style/BusinessInfo2"
                android:src="@mipmap/ic_launcher"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/handheld_identity_card_reverse"
                />
            <ImageView
                style="@style/BusinessInfo2"
                android:src="@mipmap/ic_launcher"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/handheld_identity_card_reverse"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <TextView
            android:padding="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/industry_qualification"
            android:textColor="@color/mainTextColor"
            android:textSize="16sp"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/permit_number"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/type_of_certificate"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text=""
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/type_of_certificate"
                />
            <ImageView
                style="@style/BusinessInfo2"
                android:src="@mipmap/ic_launcher"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">
            <TextView
                style="@style/BusinessInfo"
                android:text="@string/validity_of_documents"
                />
            <TextView
                style="@style/BusinessInfo2"
                android:text="2024/08/19"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
</ScrollView>
